  "authors": [

  ],
  "require": {
    "php": ">=7.4",
    "wikimedia/composer-merge-plugin": "^2.0"
  },
  "extra": {
    "installer-name" : "plugin-name",
    "merge-plugin": {
      "include": [
        "vendor/myshoppress/core/composer.ext.json",
